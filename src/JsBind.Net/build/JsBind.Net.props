<!-- build/JsBind.Net.props -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <IncludeJsBindAssets Condition="'$(IncludeJsBindAssets)' == ''">true</IncludeJsBindAssets>
    <JsBindModuleAsset Condition="'$(JsBindScriptAsset)' == 'true'">false</JsBindModuleAsset>
    <JsBindModuleAsset Condition="'$(JsBindModuleAsset)' == ''">true</JsBindModuleAsset>
    <JsBindScriptAsset Condition="'$(JsBindScriptAsset)' == ''">false</JsBindScriptAsset>
    <LinkJsBindAssets Condition="'$(LinkJsBindAssets)' == ''">false</LinkJsBindAssets>
    <LinkJsBindAssetsPath Condition="'$(LinkJsBindAssetsPath)' == ''">wwwroot\_content\JsBind.Net</LinkJsBindAssetsPath>
  </PropertyGroup>

</Project>